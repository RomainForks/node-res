{
  "name": "node-res",
  "version": "5.0.1",
  "description": "Nodejs http response builder",
  "main": "index.js",
  "files": [
    "index.js",
    "methods.js"
  ],
  "scripts": {
    "pretest": "npm run lint",
    "test:local": "FORCE_COLOR=true node bin/index.js --local",
    "test": "nyc node japaFile.js",
    "test:win": "set FORCE_COLOR=true && node bin/index.js --win",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "commit": "git-cz",
    "build": "npm run compile",
    "prepublishOnly": "npm run build",
    "lint": "tslint --project tsconfig.json",
    "clean": "del build",
    "compile": "npm run lint && npm run clean && tsc"
  },
  "author": "amanvirk",
  "license": "MIT",
  "devDependencies": {
    "@adonisjs/mrm-preset": "^1.0.14",
    "@types/node": "^10.12.0",
    "co-functional": "^0.2.1",
    "commitizen": "^3.0.2",
    "coveralls": "^3.0.2",
    "cz-conventional-changelog": "^2.1.0",
    "del-cli": "^1.1.0",
    "japa": "^2.0.5",
    "japa-cli": "^1.0.1",
    "mrm": "^1.2.1",
    "nyc": "^12.0.2",
    "pkg-ok": "^2.3.1",
    "semver": "^5.6.0",
    "supertest": "^3.3.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-eslint-rules": "^5.4.0",
    "typedoc": "^0.13.0",
    "typedoc-plugin-external-module-name": "^1.1.3",
    "typedoc-plugin-single-line-tags": "^1.0.0",
    "typescript": "^3.1.3",
    "yorkie": "^2.0.0"
  },
  "dependencies": {
    "destroy": "^1.0.4",
    "etag": "^1.8.1",
    "mime-types": "^2.1.20",
    "on-finished": "^2.3.0",
    "vary": "^1.1.2"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poppinss/node-res.git"
  },
  "keywords": [
    "node-res",
    "http",
    "response",
    "http-res",
    "response",
    "builder",
    "res",
    "builder"
  ],
  "bugs": {
    "url": "https://github.com/poppinss/node-res/issues"
  },
  "homepage": "https://github.com/poppinss/node-res#readme",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "nyc": {
    "include": [
      "index.js",
      "methods.js"
    ],
    "exclude": [
      "test"
    ],
    "extension": [
      ".ts"
    ]
  },
  "gitHooks": {
    "commit-msg": "node ./node_modules/@adonisjs/mrm-preset/validateCommit/conventional/validate.js"
  }
}
